<div
  class="flex min-h-full flex-col justify-center px-6 py-12 lg:px-8">
  <div class="sm:mx-auto sm:w-full sm:max-w-sm">
    <!-- <img class="mx-auto h-10 w-auto" src="https://tailwindui.com/img/logos/mark.svg?color=indigo&shade=600" alt="Your Company"> -->
    <h2
      class="mt-10 text-center text-2xl font-bold leading-9 tracking-tight text-indigo-600 capitalize">Sign
      in to your account</h2>
  </div>

  <div class="mt-10 sm:mx-auto sm:w-full sm:max-w-sm">
    <form class="space-y-6" name="loginForm" action="/login" method="POST">
      <div>
        <label for="email"
          class="block text-sm font-medium leading-6 text-white">Email
          address</label>
        <div class="mt-2">
          <input id="email" name="email" type="email" autocomplete="email"
            required value="Hasnain@gmail.com"
            class="block w-full rounded-md border-0 py-1.5 px-2 text-black shadow-sm ring-1 ring-inset ring-gray-300  focus:ring-[2px] focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 focus-within:outline-none">
        </div>
      </div>
      <div>
        <div class="flex items-center justify-between">
          <label for="password"
            class="block text-sm font-medium leading-6 text-white">Password</label>
          <div class="text-sm">
            <a href="#"
              class="font-semibold text-indigo-600 hover:text-indigo-500">Forgot
              password?</a>
          </div>
        </div>
        <div class="mt-2">
          <div class="relative">
            <input id="password" name="password" type="password"
              autocomplete="current-password" required value="HHfutball@7"
              class="block w-full rounded-md border-0 py-1.5 px-2 text-black shadow-sm ring-1 ring-inset ring-gray-300  focus:ring-[2px] focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6 focus-within:outline-none "
            >
            <div class="absolute top-[22%] right-3 cursor-pointer">
              <svg width="22px" height="22px" id="invisible"
                viewBox="0 0 1024.00 1024.00" fill="#333333" stroke="#333333"
                transform="rotate(0)">
                <g id="SVGRepo_bgCarrier" stroke-width="0"></g><g
                  id="SVGRepo_tracerCarrier" stroke-linecap="round"
                  stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier">
                  <defs><clippath><path fill="none"
                        d="M124-288l388-672 388 672H124z"
                        clip-rule="evenodd"></path></clippath></defs><path
                    d="M508 624a112 112 0 0 0 112-112c0-3.28-.15-6.53-.43-9.74L498.26 623.57c3.21.28 6.45.43 9.74.43zm370.72-458.44L836 122.88a8 8 0 0 0-11.31 0L715.37 232.23Q624.91 186 512 186q-288.3 0-430.2 300.3a60.3 60.3 0 0 0 0 51.5q56.7 119.43 136.55 191.45L112.56 835a8 8 0 0 0 0 11.31L155.25 889a8 8 0 0 0 11.31 0l712.16-712.12a8 8 0 0 0 0-11.32zM332 512a176 176 0 0 1 258.88-155.28l-48.62 48.62a112.08 112.08 0 0 0-140.92 140.92l-48.62 48.62A175.09 175.09 0 0 1 332 512z"></path><path
                    d="M942.2 486.2Q889.4 375 816.51 304.85L672.37 449A176.08 176.08 0 0 1 445 676.37L322.74 798.63Q407.82 838 512 838q288.3 0 430.2-300.3a60.29 60.29 0 0 0 0-51.5z"></path>
                </g>
              </svg>
              <svg class="hidden" fill="#333333" id="visible" width="22px"
                height="22px" viewBox="0 0 121.03 121.03" stroke="#333333">
                <g id="SVGRepo_bgCarrier" stroke-width="0"></g><g
                  id="SVGRepo_tracerCarrier" stroke-linecap="round"
                  stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier"> <g>
                    <path
                      d="M1.35,64.212c7.9,9.9,31.4,36.3,59.2,36.3c27.8,0,51.3-26.399,59.2-36.3c1.699-2.2,1.699-5.3,0-7.399 c-7.9-9.9-31.4-36.3-59.2-36.3c-27.8-0.1-51.3,26.3-59.2,36.2C-0.45,58.913-0.45,62.012,1.35,64.212z M60.55,36.413 c13.3,0,24,10.7,24,24s-10.7,24-24,24c-13.3,0-24-10.7-24-24S47.25,36.413,60.55,36.413z"></path>
                    <circle cx="60.55" cy="60.413" r="12"></circle> </g> </g>
              </svg>
            </div>
          </div>
        </div>
        <% if (locals.error) { %>
          <ul class="list-disc pl-4 text-sm text-red-500 mt-2">
            <li id="matcher" class="relative "><span
                class="text-[55px] absolute -top-[19px] -left-[18px]">.</span>
                <%= error %></li>
          </ul>
        <% } %>
        <ul class="list-disc pl-4 text-lg text-red-500 mt-2">
          <li id="error" class="relative hidden"><span
              class="text-[55px] absolute -top-[19px] -left-[18px]">.</span></li>
        </ul>
      </div>
      <div>
        <button type="submit"
          class="flex w-full justify-center rounded-md bg-indigo-600 px-3 py-1.5 mt-7 text-sm font-semibold leading-6 text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600">Sign
          in</button>
      </div>
    </form>
    <p class="mt-8 text-center text-sm text-gray-400">
      Not a member?
      <a href="/signup"
        class="font-semibold leading-6 text-indigo-600 hover:text-indigo-500">Register
        Your Account</a>
    </p>
  </div>
</div>